{
  "env": {
    "execution.parallelism": 1,
    "job.mode": "STREAMING",
    "checkpoint.interval": 5000
  },
  "source": [
    {
      "plugin_name": "Kafka",
      "bootstrap.servers": "172.16.11.151:9092,172.16.11.152:9092,172.16.11.153:9092",
      "topic": "ocean",
      "schema": {
        "fields": {
          "id": "BIGINT",
          "name": "STRING",
          "age": "BIGINT",
          "rate": "DOUBLE"
        }
      },
      "start_mode": "group_offsets",
      "format_error_handle_way": "fail",
      "format": "text",
      "result_table_name": "ocean",
      "consumer.group": "ocean",
      "field_delimiter": ","
    }
  ],
  "sink": [
    {
      "plugin_name": "Jdbc",
      "url": "jdbc:oracle:thin:@172.16.11.193:1521/ORCL?useInformationSchema=true&remarks=true",
      "driver": "oracle.jdbc.OracleDriver",
      "user": "wangmengting",
      "password": "wangmengting",
      "database": "WANGMENGTING",
      "batch_size": 2000,
      "primary_keys": [
        "id"
      ],
      "source_table_name": "ocean",
      "enable_upsert": true,
      "generate_sink_sql": true,
      "table": "ods_ocean",
      "fields": [
        "id",
        "name",
        "age",
        "rate"
      ]
    }
  ]
}